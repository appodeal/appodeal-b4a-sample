Sub Get_Ad_Type() As Int
	Select Case adType.SelectedIndex
		Case -1, 0
			Return Appodeal.NONE
		Case 1
			Return Appodeal.BANNER_TOP
		Case 2
			Return Appodeal.BANNER_BOTTOM
		Case 3
			Return Appodeal.INTERSTITIAL
		Case 4
			Return Appodeal.SKIPPABLE_VIDEO
		Case 5
			Return Appodeal.REWARDED_VIDEO
		Case 6
			Return Bit.Or(Appodeal.INTERSTITIAL, Appodeal.SKIPPABLE_VIDEO)
	End Select
	Return 0
End Sub

Sub init_Click
	'Appodeal.setAutoCache(Bit.Or(Appodeal.VIDEO, Appodeal.INTERSTITIAL), False) 'this is how you can 
	Appodeal.setEventHandler("Handler")
	Appodeal.setRewardedVideoCallbacks()
	Appodeal.setBannerCallbacks()
	Appodeal.setSkippableVideoCallbacks()
	Appodeal.setInterstitialCallbacks()
	'Appodeal.setNonSkippableVideoCallbacks()
	If(checkConfirm.Checked) Then
		Appodeal.confirm(Appodeal.SKIPPABLE_VIDEO)
	End If
	If(checkTesting.Checked) Then
		Appodeal.setTesting(True)
	End If
	If(checkLogging.Checked) Then
		Appodeal.setLogLevel(Appodeal.LOG_LEVEL_VERBOSE)
	End If
	If(checkDisableLocationPermissionCheck.Checked) Then
		Appodeal.disableWriteExternalStoragePermissionCheck()
	End If
	If(checkDisableWriteExternalStorageCheck.Checked) Then
		Appodeal.disableLocationPermissionCheck()
	End If
	If(checkConfirm.Checked) Then
		Appodeal.confirm(Get_Ad_Type)
	End If
	
	'Set user settings for better targeting
	Appodeal.initUserSettings()
	Appodeal.setAge(25)
	Appodeal.setBirthday("17/06/1990")
	Appodeal.setEmail("hi@appodeal.com")
	Appodeal.setGender(Appodeal.GENDER_FEMALE)
	Appodeal.setInterests("reading, games, movies, snowboarding")
	Appodeal.setOccupation(Appodeal.OCCUPATION_WORK)
	Appodeal.setRelation(Appodeal.RELATION_DATING)
	Appodeal.setAlcohol(Appodeal.ALCOHOL_NEGATIVE)
	Appodeal.setSmoking(Appodeal.SMOKING_NEGATIVE)
	Appodeal.setUserId("your_custom_user_id")
	
	Appodeal.set728x90Banners( checkDisable728x90Banners.Checked == False )
    Appodeal.setSmartBanners(checkDisableSmartBanners.Checked == False)
    Appodeal.setBannerAnimation(checkDisableBannerAnimation.Checked == False)
	Appodeal.setOnLoadedTriggerBoth(Get_Ad_Type, checkEnableTriggerOnLoadedTwice.Checked)
	
	Appodeal.setAutoCache(Get_Ad_Type, checkAutocache.Checked)
	Appodeal.initialize(appKey, Get_Ad_Type) 'use this to initialize ad types
    init.Enabled = False
	init.Text = "INITIALIZED v" & Appodeal.getVersion()
End Sub


Sub showWithPlacement_Click
	Appodeal.showWithPlacement(Get_Ad_Type, "level_end")
End Sub

Sub show_Click
	Appodeal.show(Get_Ad_Type)
End Sub

Sub isPrecache_Click
	Log("Is precache: " & Appodeal.isPrecache(Get_Ad_Type))

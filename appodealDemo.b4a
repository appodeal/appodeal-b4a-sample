Sub init_Click
	'Appodeal.setAutoCache(Bit.Or(Appodeal.VIDEO, Appodeal.INTERSTITIAL), False) 'this is how you can 
	Appodeal.setEventHandler("Handler")
	Appodeal.setRewardedVideoCallbacks()
	Appodeal.setBannerCallbacks()
	Appodeal.setInterstitialCallbacks()
	'Appodeal.setNonSkippableVideoCallbacks()
	If(checkConfirm.Checked) Then
		Appodeal.confirm(Appodeal.INTERSTITIAL)
	End If
	If(checkTesting.Checked) Then
		Appodeal.setTesting(True)
	End If
	If(checkLogging.Checked) Then
		Appodeal.setLogLevel(Appodeal.LOG_LEVEL_VERBOSE)
	End If
	Appodeal.disableWriteExternalStoragePermissionCheck()
	Appodeal.disableLocationPermissionCheck()
	
	'Set user settings for better targeting
	Appodeal.initUserSettings()
	Appodeal.setAge(25)
	Appodeal.setBirthday("17/06/1990")
	Appodeal.setEmail("hi@appodeal.com")
	Appodeal.setGender(Appodeal.GENDER_FEMALE)
	Appodeal.setInterests("reading, games, movies, snowboarding")
	Appodeal.setOccupation(Appodeal.OCCUPATION_WORK)
	Appodeal.setRelation(Appodeal.RELATION_DATING)
	Appodeal.setAlcohol(Appodeal.ALCOHOL_NEGATIVE)
	Appodeal.setSmoking(Appodeal.SMOKING_NEGATIVE)
	Appodeal.setUserId("your_custom_user_id")
	
	Appodeal.set728x90Banners(False)
    Appodeal.setSmartBanners(False)
    Appodeal.setBannerAnimation(False)
	
	Appodeal.setAutoCache(Appodeal.INTERSTITIAL, False)
	Appodeal.initialize(appKey, Appodeal.REWARDED_VIDEO) 'use this to initialize ad types
	Appodeal.initialize(appKey, Bit.Or(Appodeal.INTERSTITIAL, Appodeal.BANNER))
    init.Enabled = False
	init.Text = "INITIALIZED v" & Appodeal.getVersion()
	
	checkLogging.Enabled = False
	checkTesting.Enabled = False
	checkConfirm.Enabled = False
End Sub

Sub showInterstitial_Click
	If(interstitialState == 0) Then
		Appodeal.cache(Appodeal.INTERSTITIAL)
		interstitialState = 1
	End If
	If(interstitialState == 2) Then
		Appodeal.showWithPlacement(Appodeal.INTERSTITIAL, "level_end")
		Appodeal.setCustomBooleanRule("level_end", True)
		interstitialState = 0
	End If
End Sub

Sub rewardedVideo_Click
	If(Appodeal.isLoaded(Appodeal.REWARDED_VIDEO)) Then
		Appodeal.show(Appodeal.REWARDED_VIDEO)
	End If
End Sub

Sub showBanner_Click
	Appodeal.show(Appodeal.BANNER_BOTTOM)
End Sub

Sub hideBanner_Click
	Appodeal.hide(Appodeal.BANNER)
End Sub


	Dim interstitialState As Int = 0 '//0 - not loaded; 1 - loading; 2 - loaded'
End Sub

Sub Activity_Create(FirstTime As Boolean)
	Activity.LoadLayout("main")
	Activity.Color = Colors.Red
	

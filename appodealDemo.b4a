Sub Process_Globals
	
End Sub

Sub Globals
	Dim Appodeal As AppodealB4A
	Dim init As Button
	Dim showInterstitial As Button
	Dim showVideo As Button
	Dim rewardedVideo As Button
	Dim showVideoOrInterstitial As Button
	Dim showBanner As Button
	Dim hideBanner As Button
	'Dim showI As Boolean = False
	Dim showRW As Boolean = False
End Sub

Sub Activity_Create(FirstTime As Boolean)
	Activity.LoadLayout("main")
	
  	init.Text = "INITIALIZE"
	showInterstitial.Text = "INTERSTITIAL"
	showVideo.Text = "VIDEO"
	rewardedVideo.Text = "REWARDED VIDEO"
	showVideoOrInterstitial.Text = "VIDEO | INTERSTITIAL"
	showBanner.Text = "BANNER"
	hideBanner.Text = "HIDE BANNER"
End Sub

Sub Activity_Resume

End Sub

Sub Activity_Pause (UserClosed As Boolean)

End Sub

Sub init_Click
	Dim appKey As String = "fee50c333ff3825fd6ad6d38cff78154de3025546d47a84f"
	'Appodeal.setAutoCache(Bit.Or(Appodeal.VIDEO, Appodeal.INTERSTITIAL), False) 'use this to set Manual cache mode
	'Appodeal.initializeAdType(appKey, Bit.Or(Appodeal.VIDEO, Appodeal.INTERSTITIAL)) 'use this to init only needed ad types
	Appodeal.initialize(appKey, Appodeal.ALL) 'use this to initialize ad types
	Appodeal.setRewardedVideoCallbacks()
	Appodeal.setBannerCallbacks()
	Appodeal.setEventHandler("Handler")
	showRW = True
	init.Text = "INITIALIZED v" & Appodeal.getVersion()
End Sub

Sub showInterstitial_Click
	'showI = True 'just example how to make show once ads loaded, you can simply use Appodeal.show(Appodeal.INTERSTITIAL)
	Appodeal.show(Appodeal.INTERSTITIAL)
End Sub

Sub showVideo_Click
If(Appodeal.isLoaded(Appodeal.VIDEO)) Then
	Appodeal.show(Appodeal.VIDEO)
End If
End Sub

Sub rewardedVideo_Click
If(Appodeal.isLoaded(Appodeal.REWARDED_VIDEO)) Then
	Appodeal.show(Appodeal.REWARDED_VIDEO)
End If
End Sub

Sub showVideoOrInterstitial_Click
	Appodeal.show(Bit.Or(Appodeal.INTERSTITIAL, Appodeal.VIDEO))
End Sub

Sub showBanner_Click
	Appodeal.show(Appodeal.BANNER_BOTTOM)
End Sub

Sub hideBanner_Click
	Appodeal.hide(Appodeal.BANNER)
End Sub

Sub Handler_RewardedVideoLoaded
If (showRW) Then
	Appodeal.show(Appodeal.REWARDED_VIDEO)
	showRW = False
End If
End Sub

Sub Handler_RewardedVideoFinished(amount As Int, name As String)
	rewardedVideo.Text = "REWARD " & amount & " " & name
End Sub

Sub Handler_BannerLoaded
	Appodeal.show(Appodeal.BANNER_BOTTOM)
End Sub